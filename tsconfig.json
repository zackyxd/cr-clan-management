{
  "extends": "@tsconfig/node20/tsconfig.json",
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "target": "ES2020",
    "sourceMap": true,
    "outDir": "dist",
    "noEmitOnError": true,
    "strict": true,
    "types": ["node", "discord.js"],
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": ["src/**/*", "src/types"],
  "exclude": ["nodes_modules"]
}
