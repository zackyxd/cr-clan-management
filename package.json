{
  "name": "cr-management-discord-bot",
  "version": "1.0.0",
  "main": "dist/bot.js",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "lint": "eslint",
    "migrate:dev:up": "dotenv -e .env.dev npx node-pg-migrate up",
    "migrate:dev:down": "dotenv -e .env.dev -- npx node-pg-migrate down 1",
    "start:dev": "npm run clean && tsc && NODE_ENV=dev node dist/bot.js",
    "start:prod": "npm run clean && tsc && NODE_ENV=prod node dist/bot.js",
    "test": "node --experimental-vm-modules node_modules/.bin/jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@tsconfig/node20": "^20.1.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.14",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "dotenv-cli": "^8.0.0",
    "globals": "^16.3.0",
    "jest": "^30.0.4",
    "nsx": "^0.0.1",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript-eslint": "^8.37.0"
  },
  "dependencies": {
    "@types/pg": "^8.15.4",
    "discord.js": "^14.21.0",
    "dotenv": "^17.2.0",
    "dotenv-flow": "^4.1.0",
    "node-pg-migrate": "^8.0.3",
    "pg": "^8.16.3",
    "pg-format": "^1.0.4",
    "pg-pool": "^3.10.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
